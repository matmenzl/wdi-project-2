<br>
<div class="row">
  <div class="medium-12 columns">
    <div class="card">
<div class="image"><!-- 
  <h1>Your Neighbourhood <%= current_user.zip %> <%= current_user.city %></h1> -->
  <%= image_tag "http://maps.google.com/maps/api/staticmap?size=1200x100&sensor=false&zoom=12&scale=4&markers=#{current_user.latitude}%2C#{current_user.longitude}" %>
</div>
</div>
</div>
</div>
<br>
<div class="row spacer" data-equalizer>
<% @users.each do |user| %>
<div class="small-5 medium-4 large-3 columns end">
  <div class="panel" data-equalizer-watch>
   <article class="article-card">
     <div class="card-content">
       <%= link_to image_tag(user.user_image.thumb.url), user.user_image.url, class: 'userimage' %>
       <p class="post-author">
         <%= user.first_name %>
         <%= user.last_name %>
       </p>
       <p>
         <div class="actions">
         <button class="button tiny round"><i class="fa fa-user"></i> <%= link_to user.try(:first_name), user %></button>
         </div>
       </p>
     </div>
   </article>
 </div>
</div>
<% end %>
</div>


